{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-standard", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "jsx-boolean-value": false,
    "no-empty": false,
    "no-use-before-declare": true,
    "react/destructuring-assignment": true,
    "label-position": false,
    "no-unused-expression": [true, "allow-fast-null-checks"],
    "no-return-await": false,
    "no-any": false,
    "no-var-requires": false,
    "no-shadowed-variable": false,
    "no-console": false,
    "jsx-key": false,
    "no-extra-boolean-cast": false,
    "radix": false,
    "variable-name": false,
    "handle-callback-err": false,
    "no-duplicate-imports": false,
    "class-name": false,
    "triple-equals": false,
    "curly": false,
    "prefer-for-of": false,
    "no-string-literal": false,
    "jsx-self-close": false
  },
  "linterOptions": {
    "exclude": ["src/serviceWorker.ts"]
  }
}
